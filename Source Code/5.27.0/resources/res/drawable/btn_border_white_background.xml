<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dp1" android:color="@color/white_50"/>
            <corners android:radius="@dimen/dp8"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/white_20"/>
            <corners android:radius="@dimen/dp8"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dp1" android:color="@color/white"/>
            <corners android:radius="@dimen/dp8"/>
        </shape>
    </item>
</selector>
